{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "productName": "MIU Player",
  "version": "1.0.0",
  "identifier": "com.miu.player",
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "cp frontend/* dist/",
    "frontendDist": "./dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "MIU Player",
        "width": 450,
        "height": 650,
        "minWidth": 400,
        "minHeight": 500,
        "center": true,
        "decorations": true,
        "alwaysOnTop": false,
        "skipTaskbar": false
      }
    ],
    "security": {
      "csp": null,
      "capabilities": [
        {
          "identifier": "main-capability",
          "description": "Capability for main window",
          "windows": ["main"],
          "permissions": [
            "core:event:default",
            "core:event:allow-listen",
            "core:event:allow-emit",
            "notification:default"
          ]
        }
      ]
    }
  },
  "bundle": {
    "active": true,
    "category": "Music",
    "copyright": "Â© 2024 MIU Team",
    "shortDescription": "MIU Music Player",
    "longDescription": "Cross-platform desktop music player for MIU music bot",
    "targets": [],
    "icon": [
      "icons/tray-icon.ico",
      "icons/icon.png"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  },
  "plugins": {}
}