    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.12.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.1",
    "youtube-music-api": "^1.0.6",
