{
  "name": "miu-backend",
  "version": "1.0.0",
  "description": "Discord music bot with web interface - Backend",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio --port 5000",
    "prisma:create-admin": "tsx scripts/create-admin.ts",
    "docker:up": "docker-compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.yml down",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "discord",
    "music",
    "bot",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/rest": "^2.4.3",
    "@discordjs/voice": "^0.18.0",
    "@prisma/client": "^5.10.2",
    "@trpc/server": "^10.45.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/sharp": "^0.31.1",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "discord-api-types": "^0.37.119",
    "discord.js": "^14.18.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.17.3",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
    "googleapis": "^144.0.0",
    "ioredis": "^5.5.0",
    "jsonwebtoken": "^9.0.2",
    "play-dl": "^1.9.7",
    "prom-client": "^15.1.0",
    "redis": "^4.7.0",
    "sharp": "^0.33.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.12.0",
    "winston-daily-rotate-file": "^5.0.0",
    "youtube-music-api": "^1.0.6",
