{
  "name": "miu-backend",
  "version": "1.0.0",
  "description": "Discord music bot with web interface - Backend",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "build-no-lint": "tsc --noEmit false",
    "start": "node dist/index.js",
    "stop": "./scripts/kill-server.sh",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "dotenv -e .env.development -- prisma studio --port 5000",
    "prisma:create-admin": "tsx scripts/create-admin.ts",
    "docker:up": "docker-compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.yml down",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "discord",
    "music",
    "bot",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@discordjs/builders": "^1.7.0",
    "@discordjs/collection": "^2.0.0",
    "@discordjs/core": "^1.1.1",
    "@discordjs/rest": "^2.2.0",
    "@discordjs/voice": "^0.18.0",
    "@prisma/client": "^5.22.0",
    "@trpc/server": "^10.45.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/entities": "^2.0.2",
    "@types/sharp": "^0.31.1",
    "@types/ws": "^8.5.14",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "discord-api-types": "^0.37.119",
    "discord.js": "^14.18.0",
    "entities": "^6.0.0",
    "execa": "^5.1.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.17.3",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
    "googleapis": "^144.0.0",
    "ioredis": "^4.28.5",
    "jsonwebtoken": "^9.0.2",
